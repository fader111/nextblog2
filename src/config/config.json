{
  "site": {
    "title": "MLtrack",
    "base_url": "https://nextplate.netlify.app",
    "base_path": "/",
    "trailing_slash": false,
    "favicon": "/images/favicon.png",
    "logo": "/images/logo2.png",
    "logo_darkmode": "/images/logo-darkmode.png",
    "logo_width": "110",
    "logo_height": "63",
    "logo_text": "MLtrack"
  },

  "settings": {
    "search": true,
    "sticky_header": true,
    "theme_switcher": true,
